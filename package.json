{
  "name": "lens-open-actions",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "forge build --skip .t.sol .s.sol --via-ir",
    "build:hardhat": "npx hardhat clean && npx hardhat compile",
    "test": "forge test",
    "test:gas": "forge test -vvv --gas-report",
    "deploy:testnet": "npx hardhat testnet-deploy-actions --network mumbai",
    "simulate:deploy-creator": ". ./.env && forge script script/DeployZoraLzCreator.s.sol:DeployZoraLzCreator --rpc-url ${MAINNET_RPC_URL} -vvvv --skip .t.sol --legacy"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-ethers": "^3.0.4",
    "@nomicfoundation/hardhat-foundry": "^1.0.2",
    "@nomicfoundation/hardhat-verify": "^2.0.3",
    "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers",
    "@openzeppelin/hardhat-upgrades": "1.28.0",
    "@tenderly/hardhat-tenderly": "^1.7.7",
    "dotenv": "^16.3.1",
    "ethers": "^5.7.1",
    "hardhat": "^2.19.2",
    "hardhat-deploy": "^0.11.34",
    "hardhat-preprocessor": "^0.1.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3",
    "yargs": "^17.7.2"
  },
  "dependencies": {
    "@lens-protocol/metadata": "^1.1.5",
    "@openzeppelin/contracts": "4.8.0",
    "@openzeppelin/contracts-upgradeable": "4.8.0",
    "promise-limit": "^2.7.0",
    "zod": "^3.22.4"
  }
}
